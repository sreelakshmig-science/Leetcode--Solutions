class Solution:
    def sortArrayByParity(self, nums: L[int]) -> L[int]:
        size = len(nums)
        i = 0
        end = size - 1

        while i <= end:
            if nums[i] % 2 == 1:
                nums[i], nums[end] = nums[end], nums[i]
                end -= 1
                continue
            i += 1

        return nums        